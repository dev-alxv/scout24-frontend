{
  "name": "scout24-frontend-app",
  "version": "0.0.1",
  "buildNum": "",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build --prod  && ncu",
    "buildDev": "ng build --configuration=prodDev && ncu",
    "test": "ng test --source-map=false --code-coverage",
    "lint": "ng lint",
    "e2e:cypress": "cypress open",
    "test:pipeline": "ng test --source-map=false --watch=false --browsers=ChromeHeadless",
    "ci:test": "npm run test:pipeline",
    "ci:build-sandbox": "ng build --configuration=prodDev --base-href /immoviewer-virtual-tours/ --output-path dist/immoviewer-virtual-tours",
    "ci:build": "ng build --prod --base-href /immoviewer-virtual-tours/ --output-path dist/immoviewer-virtual-tours",
    "ci:test-e2e": "npm run ci:build && ./node_modules/.bin/run-p --race ci:serve ci:cypress",
    "ci:serve": "http-server ./dist -a localhost -p 4200 -c-1",
    "ci:cypress": "npx cypress run --config video=false --browser chrome --headless",
    "ci:start": "npm run ci:test && npm run ci:test-e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/color-picker": "^5.0.2",
    "@angular/animations": "~11.2.3",
    "@angular/cdk": "^11.2.2",
    "@angular/common": "~11.2.3",
    "@angular/compiler": "~11.2.3",
    "@angular/core": "~11.2.3",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.2.3",
    "@angular/localize": "^11.2.3",
    "@angular/material": "^11.2.2",
    "@angular/platform-browser": "~11.2.3",
    "@angular/platform-browser-dynamic": "~11.2.3",
    "@angular/router": "~11.2.3",
    "@material-ui/icons": "^4.11.2",
    "@ngneat/until-destroy": "^8.0.4",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "flag-icon-css": "^3.5.0",
    "ngx-material-file-input": "^2.1.1",
    "ngx-pagination": "^5.0.0",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.2",
    "@angular/cli": "~11.2.2",
    "@angular/compiler-cli": "~11.2.3",
    "@angular/language-service": "~11.2.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "cypress": "^7.3.0",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.4",
    "npm-check-updates": "^11.5.13",
    "npm-run-all": "^4.1.5",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.7"
  }
